{
    "version": 2,
    "builds": [
        {
            "src": "server/index.js",
            "use": "@vercel/node"
        },
        {
            "src": "new-e-solution/package.json",
            "use": "@vercel/static-build",
            "config": {
                "distDir": "dist"
            }
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "/server/index.js"
        },
        {
            "src": "/(.*)",
            "dest": "/new-e-solution/$1"
        }
    ],
    "rewrites":[
        [
            {
                "source":"/(.*)",
                "destination":"/new-e-solution/dist/index.html"
            }
            

        ]
    ]
}