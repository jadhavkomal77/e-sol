{
  "version": 2,
  "builds": [
    {
      "src": "server/index.js",
      "use": "@vercel/node"
    },
    {
      "src": "new-e-solution/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/server/index.js"
    },
    {
      "src": "/site/(.*)",
      "dest": "/new-e-solution/index.html"
    },
    {
      "src": "/(.*)",
      "dest": "/new-e-solution/index.html"
    }
  ]
}
